{
  "family": "myapp-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::368903465629:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::368903465629:role/ecsTaskExecutionRole", 
  "containerDefinitions": [
    {
      "name": "myapp",
      "image": "368903465629.dkr.ecr.ap-southeast-1.amazonaws.com/myapp-repo:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
